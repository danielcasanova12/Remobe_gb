# Arquivo de exemplo para configurações de ambiente
# Copie para .env e ajuste conforme necessário

# Modo de desenvolvimento (true/false)
DEBUG=false

# Nível de log (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Configurações de arquivo de log
LOG_TO_FILE=true
LOG_FILE_MAX_SIZE=10485760  # 10MB em bytes
LOG_FILE_BACKUP_COUNT=5     # Número de arquivos de backup
